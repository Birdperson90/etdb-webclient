{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@etdb/*": ["app/*"]
        },
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "target": "es5",
        "typeRoots": ["node_modules/@types"],
        "types": ["gapi", "gapi.auth2"],
        "lib": ["es2017", "dom"]
    }
}
