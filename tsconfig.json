{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./src",
        "importHelpers": true,
        "paths": {
            "@etdb/*": [
                "app/*"
            ]
        },
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "target": "es5",
        "typeRoots": [
            "node_modules/@types"
        ],
        "types": [
            "gapi",
            "gapi.auth2",
            "fb"
        ],
        "lib": [
            "es2017",
            "dom"
        ]
    }
}